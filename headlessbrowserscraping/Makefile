all:
	@echo Running headlessfirefox.py on Python 3...
	python3 headlessbrowserscraping.py
	@echo Done! Exiting...

usebothbrowsers:
	@echo Running headlessfirefox.py on Python 3...
	python3 headlessbrowserscraping.py --browser chrome --iterations 2 --wait_time 2
	python3 headlessbrowserscraping.py --browser firefox --iterations 2 --wait_time 2
	@echo Done! Exiting...